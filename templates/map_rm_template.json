    {
        "srce": "pncc",
        "sequence": 1,
        "name": "Currency",
        "description": "pull out currency indicators from description of misc items and map",
        "defn": [
            {
                "key": "{Description}",
                "field": "ini",
                "regex": "([\\w].*?)(?=$| -|\\s[0-9].*?|\\s[\\w/]+?:)",
                "retain": "y",
                "map": "y"
            },
            {
                "key": "{Description}",
                "field": "curr1",
                "regex": ".*(DEBIT|CREDIT).*(USD|CAD).*(?=DEBIT|CREDIT).*(?=USD|CAD).*",
                "retain": "y",
                "map": "y"
            },
            {
                "key": "{Description}",
                "field": "curr2",
                "regex": ".*(?=DEBIT|CREDIT).*(?=USD|CAD).*(DEBIT|CREDIT).*(USD|CAD).*",
                "retain": "y",
                "map": "y"
            }
        ],
        "where": [
            {
                "Transaction": "Miscellaneous Credits"
            },
            {
                "Transaction": "Miscellaneous Debits"
            }
        ],
        "function": "extract"
